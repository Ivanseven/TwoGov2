
<ul class="side-navbar">
  <li class="active"><a>User Profile</a></li>
  <li><%= link_to "Your Listing", post_listing_path %></li>
  <li><%= link_to "Edit Profile", edit_user_registration_path %></li>
  <li><%= link_to "Request", invitations_path %></li>
</ul>


<h1>Welcome to Two-go</h1>

<div>
	<% if current_user.avatar.url.present? %>
		<%= image_tag(current_user.avatar.thumb.url) %>
	<% end %>
</div>


<p>Welcome, <%= current_user.first_name %></p>

<p>First Name: <%= current_user.first_name%></p>
<p>Last Name: <%= current_user.last_name%></p>
<p>Gender: <%= current_user.gender%></p>
<p>Date of Birth: <%= current_user.date_of_birth%></p>
<p>Email: <%= current_user.email%></p>

